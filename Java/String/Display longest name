class GFG {
    String longest(String names[], int n) {
        int max=0;
        for (int i=0;i<n;i++)
        {
            max=(int)Math.max(max,names[i].length());
        }
        String ans="";
        for (int i=0;i<n;i++)
        {
            if(names[i].length()==max)
            {
                return names[i];
            }
        }
        return "";
    }
}
